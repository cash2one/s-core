<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byName">
    <bean id="providerManager" class="com.seo.auto.runner.provider.MockProviderManagerImpl"/>

    <bean id="wordProvider" class="com.seo.text.words.TextFileWordProvider"/>

    <bean id="responseParser" class="com.seo.antigate.connector.response.ResponseParserImpl"/>

    <bean id="antigateConnector" class="com.seo.antigate.connector.AntigateConnectorImpl">
        <property name="antigateKey" value="76094ffba7c0f6bf64319a74fb3b4e51"/>
        <property name="antigateInputUrl" value="http://www.antigate.com/in.php"/>
    </bean>

    <bean id="captchaService" class="com.seo.antigate.AntigateCaptchaServiceImpl"/>

    <bean id="autoConfigAdaptor" class="com.seo.core.facade.auto.AutoConfigAdaptorImpl" scope="prototype"/>
    <bean id="configFacade" class="com.seo.auto.facade.ConfigFacadeImpl" scope="prototype">
        <lookup-method name="createCommandProcessor" bean="commandProcessor"/>
    </bean>
    <bean id="projectParser" class="com.seo.auto.parser.ProjectParserImpl"/>
    <bean id="commandProcessor" class="com.seo.auto.command.CommandProcessorImpl" scope="prototype"/>
    
</beans>